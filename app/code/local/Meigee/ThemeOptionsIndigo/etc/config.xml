				<sort_order>9999</sort_order>
				<children>
					<theme_options translate="title" module="ThemeOptionsIndigo">
						<title>Theme Options v1.3</title>
						<sort_order>1</sort_order>
						<children>
							<general_options translate="title" module="ThemeOptionsIndigo">
								<title>General Options</title>
								<sort_order>1</sort_order>
								<action>adminhtml/system_config/edit/section/meigee_indigo_general</action>
							</general_options>
							<design_options translate="title" module="ThemeOptionsIndigo">
								<title>Design Options</title>
								<sort_order>2</sort_order>
