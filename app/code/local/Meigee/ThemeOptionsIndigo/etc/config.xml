							</design_options>
							<productpage_options translate="title" module="ThemeOptionsIndigo">
								<title>Product Page Options</title>
								<sort_order>3</sort_order>
								<action>adminhtml/system_config/edit/section/meigee_indigo_productpage</action>
							</productpage_options>
							<sidebar_options translate="title" module="ThemeOptionsIndigo">
								<title>Sidebar Options</title>
								<sort_order>4</sort_order>
								<action>adminhtml/system_config/edit/section/meigee_indigo_sidebar</action>
							</sidebar_options>
							<headerslider_options translate="title" module="ThemeOptionsIndigo">
								<title>Header Slider</title>
								<sort_order>5</sort_order>
								<action>adminhtml/system_config/edit/section/meigee_indigo_headerslider</action>
							</headerslider_options>
							<bgslider_options translate="title" module="ThemeOptionsIndigo">
								<title>Background Slider</title>
								<sort_order>6</sort_order>
								<action>adminhtml/system_config/edit/section/meigee_indigo_bgslider</action>
							</bgslider_options>
						</children>
					</theme_options>
					<restore_theme translate="title" module="ThemeOptionsIndigo">
						<title>Restore Defaults</title>
						<sort_order>1</sort_order>
						<action>themeoptionsindigo/restore/index</action>
					</restore_theme>
					<activate_theme translate="title" module="ThemeOptionsIndigo">
						<title>Activate Theme</title>
						<sort_order>1</sort_order>
						<action>themeoptionsindigo/activation/index</action>
					</activate_theme>
				</children>
			</meigee>
		</menu>
